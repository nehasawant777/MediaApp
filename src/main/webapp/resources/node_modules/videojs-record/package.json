    "docs": "npm-run-all docs:*",
    "docs:api": "jsdoc src/js -r -d docs/api",
    "lint": "npm-run-all lint:*",
    "lint:html": "htmlhint examples",
    "lint:js": "jscs --config .jscsrc src/js",
    "lint:json": "jsonlint-cli src/lang/*.json font/*.json",
